e\";\n  remeber_task.value = task;\n}\n\nfunction openAddModal() {\n  show_task_modal.value = true;\n  action.value = \"Add\";\n  remeber_task.value = {\n    title: \"\",\n    schedule: \"\",\n    status: \"\",\n  };\n}\n\nfunction deleteTask(task) {\n  remeber_task.value = task;\n  show_confirmation_modal.value = true;\n}\n\nconst tasks = computed(() => store.state.tasks.tasks_list);\n</script>\n\n<style lang=\"scss\" scoped>\nthead {\n  tr {\n    @apply border-b border-gray-300;\n  }\n  td {\n    @apply pl-4 py-2;\n  }\n}\ntbody {\n  tr {\n    @apply border-b border-gray-300;\n  }\n  td {\n    @apply pl-4 py-2 bg-white text-gray-600 text-sm;\n  }\n}\n\n.status-Notstarted {\n  @apply w-fit m-auto font-bold bg-blue-100 text-blue-400 rounded-lg py-1 px-2;\n}\n.status-Inprogress {\n  @apply w-fit m-auto font-bold bg-green-100 text-green-400 rounded-lg py-1 px-2;\n}\n.status-Finished {\n  @apply w-fit m-auto font-bold bg-red-100 text-red-400 rounded-lg py-1 px-2;\n}\n</style>\n"],"mappings":"AAkEA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,SAAS,MAAM,iBAAiB;AACvC,OAAOC,iBAAiB,MAAM,yBAAyB;AACvD,SAASC,QAAQ,EAAEC,UAAU,EAAEC,SAAS,QAAQ,2BAA2B;;;;;;;IAE3E,MAAMC,KAAK,GAAGN,QAAQ,CAAC,CAAC;IACxB,MAAMO,eAAe,GAAGV,GAAG,CAAC,KAAK,CAAC;IAClC,MAAMW,uBAAuB,GAAGX,GAAG,CAAC,KAAK,CAAC;IAC1C,MAAMY,MAAM,GAAGZ,GAAG,CAAC,KAAK,CAAC;IACzB,MAAMa,YAAY,GAAGb,GAAG,CAAC,CAAC,CAAC,CAAC;IAE5BC,SAAS,CAAC,MAAM;MACdQ,KAAK,CAACK,QAAQ,CAAC,oBAAoB,CAAC;IACtC,CAAC,CAAC;IAEF,SAASC,UAAUA,CAAA,EAAG;MACpBL,eAAe,CAACM,KAAK,GAAG,KAAK;MAC7BJ,MAAM,CAACI,KAAK,GAAG,KAAK;MACpBH,YAAY,CAACG,KAAK,GAAG;QACnBC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE;MACV,CAAC;IACH;IAEA,SAASC,sBAAsBA,CAAA,EAAG;MAChCT,uBAAuB,CAACK,KAAK,GAAG,KAAK;MACrCH,YAAY,CAACG,KAAK,GAAG;QACnBC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE;MACV,CAAC;IACH;IAEA,SAASE,eAAeA,CAACC,IAAI,EAAE;MAC7BZ,eAAe,CAACM,KAAK,GAAG,IAAI;MAC5BJ,MAAM,CAACI,KAAK,GAAG,QAAQ;MACvBH,YAAY,CAACG,KAAK,GAAGM,IAAI;IAC3B;IAEA,SAAS